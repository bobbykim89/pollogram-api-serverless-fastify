{
  "name": "pollogram-fastify-cdk",
  "version": "0.1.0",
  "bin": {
    "pollogram-fastify-cdk": "bin/pollogram-fastify-cdk.js"
  },
  "scripts": {
    "dev:local": "dotenv -e .env -- npm  --prefix ./app run dev",
    "dev:local:watch": "dotenv -e .env -- npm --prefix ./app run dev:watch",
    "db:local:rm": "docker compose rm local-db -s -f -v",
    "db:local:up": "docker compose up local-db -d",
    "db:local:restart": "dotenv -e .env -- npm run db:local:rm && npm run db:local:up && npm --prefix ./app run db:migrate:deploy",
    "db:migrate:create": "dotenv -e .env -- npm --prefix ./app run db:migrate:create",
    "db:migrate:deploy": "dotenv -e .env -- npm --prefix ./app run db:migrate:deploy",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:bootstrap": "dotenv -e .env -- cdk bootstrap",
    "cdk:deploy:prod": "dotenv -e .env -e .env.prod -- cdk deploy",
    "cdk:destroy:prod": "dotenv -e .env -e .env.prod -- cdk destroy",
    "cdk:deploy:dev": "dotenv -e .env -e .env.dev -- cdk deploy",
    "cdk:destroy:dev": "dotenv -e .env -e .env.dev -- cdk destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.174.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.174.0",
    "constructs": "^10.0.0"
  }
}
